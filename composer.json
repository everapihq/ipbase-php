{
    "name": "everapi/ipbase-php",
    "description": "A PHP wrapper for the ipbase.com API",
    "keywords": [
        "ipbase.com",
        "ipbase-php",
        "geolocation",
        "api",
        "ip",
        "ipbase"
    ],
    "homepage": "https://github.com/everapihq/ipbase-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Lukas Struber",
            "email": "lukas.struber@everapi.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Ipbase\\Ipbase\\": "src"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
